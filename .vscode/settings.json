{
    "python.analysis.completeFunctionParens": true,
    "python.analysis.typeCheckingMode": "basic",
    "yaml.schemas": {
    
    },
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.detectIndentation": false,
    "python.analysis.extraPaths": ["${workspaceRoot}/api/api"],
    "[python]": {
      "gitlens.codeLens.symbolScopes": [
        "!Module"
      ],
      "editor.wordBasedSuggestions": false,
      "editor.tabSize": 4
    },
    "[vue]": {
      "gitlens.codeLens.scopes": [
        "document"
      ],
      "editor.tabSize": 2
    },
    "[javascript]": {
      "editor.tabSize": 2
    },
    "search.exclude": {
      "**/node_modules": true,
      "**/bower_components": true,
      "**/*.code-search": true,
      "**/venv": true, 
      "**/node_modules/**": true,
      "chunk-*": true,
      "frontend/dist/**": true,
      ".git/**": true
    }
}